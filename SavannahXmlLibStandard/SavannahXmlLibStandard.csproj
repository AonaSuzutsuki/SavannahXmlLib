<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net48</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <Authors>Aona Suzutsuki</Authors>
    <AssemblyVersion>1.0.4.4</AssemblyVersion>
    <FileVersion>1.0.4.4</FileVersion>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RootNamespace>SavannahXmlLib</RootNamespace>
    <AssemblyName>SavannahXmlLib</AssemblyName>
    <Company>Kimama Lab.</Company>
    <Copyright>Copyright © AonaSuzutsuki 2020</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.3</Version>
    <Description>Xml wrapper library for SavannahManager.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommonCoreLib" Version="1.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
